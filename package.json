    "@types/google-cloud__storage": "^2.3.1",
    "@types/invariant": "^2.2.37",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/validator": "^13.11.8",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "babel-jest": "^29.7.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
